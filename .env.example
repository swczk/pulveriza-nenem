# === ENVIRONMENT TEMPLATE ===
# Copie este arquivo para .env e configure os valores

# PostgreSQL Database Configuration
POSTGRES_DB=tacdb
POSTGRES_USER=tacdb_owner
POSTGRES_PASSWORD=your_postgres_password
SPRING_DATASOURCE_URL=jdbc:postgresql://postgres:5432/tacdb
SPRING_DATASOURCE_USERNAME=tacdb_owner
SPRING_DATASOURCE_PASSWORD=your_postgres_password

# MongoDB Configuration
MONGO_ROOT_USERNAME=admin
MONGO_ROOT_PASSWORD=your_mongo_password
MONGO_DATABASE=pulverizacao
MONGO_URI=mongodb://admin:your_mongo_password@mongodb:27017

# AWS Cognito Configuration
AWS_COGNITO_REGION=sa-east-1
AWS_COGNITO_URL=https://cognito-idp.sa-east-1.amazonaws.com
AWS_COGNITO_USER_POOL_ID=your_user_pool_id
AWS_COGNITO_CLIENT_ID=your_client_id
AWS_COGNITO_CLIENT_SECRET=your_client_secret

# Application Configuration
SPRING_PROFILES_ACTIVE=docker
PORT=8080

# Frontend Configuration (Development)
VITE_API_URL=http://localhost:8080
VITE_GRAPHQL_URL=http://localhost:8081/graphql
VITE_AUTH_URL=http://localhost:8080
VITE_APP_NAME=Sistema de Pulverização

# Database Management Tools
MONGOEXPRESS_LOGIN=admin
MONGOEXPRESS_PASSWORD=pass